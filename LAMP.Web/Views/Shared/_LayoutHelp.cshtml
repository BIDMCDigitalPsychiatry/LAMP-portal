@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="icon" href="~/Content/Default/images/favicon.png" type="image/png">
    <title>LAMP</title>
    @Styles.Render("~/Content/Default/css/lampcss")
    @RenderSection("page_css", required: false)
    <script>
        var appPath = '@Url.Content("~/")';
    </script>
</head>
<body>
    <nav class="navbar navbar-fixed-top">
        <a href="@Url.Action("Users", "UserAdmin")" class="logo"><img src="~/Content/Default/images/logo_inner.png" alt="Lamp"></a>
    </nav>
    <div class="page-content">
        @RenderBody()
    </div>
    <footer><p class="copy">© 2017 LAMP. All rights reserved.</p></footer>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/javascripts")
    @RenderSection("page_scripts", required: false)
</body>
</html>


